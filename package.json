{
    "scripts": {
        "dfx_install": "DFX_VERSION=0.15.1 sh -ci \"$(curl -fsSL https://sdk.dfinity.org/install.sh)\"",
        "replica_start": "dfx start --background",
        "replica_stop": "dfx stop",
        "canister_deploy_local": "dfx deploy FlipCoinContract",
        "canister_deploy_mainnet": "dfx deploy --network ic FlipCoinContract",
        "canister_uninstall": "dfx canister uninstall-code FlipCoinContract",
        "canister_call_get_flip_history": "dfx canister call FlipCoinContract getFlipHistory",
        "canister_call_flip_coin": "dfx canister call FlipCoinContract flipCoin "
    },
    "dependencies": {
        "azle": "0.19.0"
    }
}

